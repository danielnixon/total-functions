{
  "name": "total-functions",
  "version": "0.0.1",
  "description": "A collection of total functions to replace TypeScript's built-in partial functions.",
  "main": "dist",
  "repository": "https://github.com/danielnixon/total-functions.git",
  "author": "Daniel Nixon <dan.nixon@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/experimental-utils": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-functional": "^1.0.0-rc.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-sonarjs": "^0.4.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "type-coverage": "^2.2.0",
    "typescript": "^3.6.2"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "type-coverage": "type-coverage --strict --detail --ignore-catch",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "release": "yarn build && yarn lint && yarn type-coverage && yarn publish"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
