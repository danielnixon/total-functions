{
  "name": "total-functions",
  "version": "3.0.0",
  "description": "A collection of total functions to replace TypeScript's built-in partial functions.",
  "main": "dist",
  "repository": "https://github.com/danielnixon/total-functions.git",
  "author": "Daniel Nixon <dan.nixon@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@stryker-mutator/core": "^3.2.4",
    "@stryker-mutator/jest-runner": "^3.2.4",
    "@stryker-mutator/typescript": "^3.2.4",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/experimental-utils": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-total-functions": "^1.4.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "type-coverage": "^2.7.1",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "type-coverage": "type-coverage --strict --detail --ignore-catch",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "release": "yarn build && yarn lint && yarn type-coverage && yarn publish"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
