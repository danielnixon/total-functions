{
  "name": "total-functions",
  "version": "0.1.0",
  "description": "A collection of total functions to replace TypeScript's built-in partial functions.",
  "main": "dist",
  "repository": "https://github.com/danielnixon/total-functions.git",
  "author": "Daniel Nixon <dan.nixon@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@stryker-mutator/core": "^2.4.0",
    "@stryker-mutator/jest-runner": "^2.4.0",
    "@stryker-mutator/typescript": "^2.4.0",
    "@types/jest": "^25.1.2",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/experimental-utils": "^2.19.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "jest": "^25.1.0",
    "patch-package": "^6.2.0",
    "postinstall-postinstall": "^2.0.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "type-coverage": "^2.4.0",
    "typescript": "^3.8.2"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "type-coverage": "type-coverage --strict --detail --ignore-catch",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "release": "yarn build && yarn lint && yarn type-coverage && yarn publish",
    "postinstall": "patch-package"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
